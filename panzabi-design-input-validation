if ( $this->currentPage < $this->pages ) {
        // dd(count(array_filter($this->design_fields)));
        // dd( array_filter($this->design_fields));
        dd( array_filter($this->designs_check));
        if ($this->currentPage!=2) {
            $this->currentPage++;
        }elseif ($this->currentPage==2) {
            if ( count($this->designs_check) > 0 ) {
                $filterOne = array_filter($this->designs_check);
                $filterTwo = array_filter($this->design_fields);
                // dd($filterOne);
                // dd(count($filterOne).'=='.count($filterTwo));
                if(count($filterOne)>count($filterTwo)){
                    foreach(array_keys(array_filter($filterOne)) as $item){
                        
                        if (in_array($item,array_keys($filterTwo), true ) ==false) {
                            $this->design_fields[$item]= ' ';
                        }
                    }
                    if(count($filterOne)== array_filter($this->design_fields)){
                        $this->currentPage++;
                    }elseif (count($filterOne)<array_filter($this->design_fields)) {
                        $this->dispatchBrowserEvent('alert', ['custom'=>"",'message' => "লাল রঙের বক্স গুলো <i class='fa fa-check text-success'></i> দিন! </span>",'effect'=>'warning']);
                    }
                    // dd(count($filterOne).'=='.count($filterThree));
                }elseif(count($filterOne)==count($filterTwo)){
                    $this->currentPage++;
                }
                // elseif (count($filterOne)<array_filter($this->design_fields)) {
                //     $this->dispatchBrowserEvent('alert', ['custom'=>"",'message' => "লাল রঙের বক্স গুলো <i class='fa fa-check text-success'></i> দিন! </span>",'effect'=>'warning']);
                // }
            }elseif (count($this->designs_check) < count(array_filter($this->design_fields))) {
                $this->dispatchBrowserEvent('alert', ['custom'=>"",'message' => "লাল রঙের বক্স গুলো <i class='fa fa-check text-success'></i> দিন! </span>",'effect'=>'warning']);
            }
            else{
                $this->dispatchBrowserEvent('alert', ['custom'=>"",'message' => "কিছু ডিজাইন যুক্ত করুণ!",'effect'=>'warning',]);
            }  
        }
        // elseif()
        
    }else{$this->currentPage=$this->currentPage;}
